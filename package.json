    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.2.0"
  }
